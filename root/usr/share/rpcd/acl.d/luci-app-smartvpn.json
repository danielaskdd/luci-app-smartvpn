{
	"luci-app-smartvpn": {
		"description": "Grant access to LuCI app smartvpn",
		"write": {
			"uci": [ "smartvpn" ],
			"cgi-io": [ "upload" ],
			"file": {
				"/etc/smartvpn/user*": [ "write" ]
			}
		},
		"read": {
			"uci": [ "smartvpn" ],
			"cgi-io": [ "download", "exec" ],
			"file": {
				"/etc/smartvpn/user*": [ "read" ],
				"/usr/sbin/smartvpn.sh *": [ "exec" ],
				"/etc/init.d/smartvpn *" : [ "exec" ],
				"/var/run/smartvpn.work" : [ "read" ]
			}
		}
	}
}
